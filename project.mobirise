          "_customHTML": "<section data-bs-version=\"5.1\" class=\"content11\" group=\"Content\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" name=\"textWidth\" title=\"Width\" min=\"4\" max=\"12\" value=\"10\" step=\"1\">\n        <header>Background</header>\n        <input type=\"color\" title=\"Color\" name=\"bgColor\" value=\"#f2f2f2\">\n    </mbr-parameters>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"ptop1 list mbr-fonts-style\" mbr-theme-style=\"display-7\">\n    <!-- start communications -->\n\t<div class=\"area\">\n    <h3>Communications<small>&nbsp;&nbsp;(texting, calling, etc)</small></h3>\n    <div class=\"box\">\n          <a href=\"https://getsession.org/\" target=\"_blank\"><u>Session</u></a>&nbsp;&nbsp;\n\n          <a href=\"https://element.io\" target=\"_blank\"><u>Element</u></a>&nbsp;&nbsp;\n\n          <a href=\"https://status.im/\" target=\"_blank\"><u>Status</u></a>&nbsp;&nbsp;\n        \n          <a href=\"https://signal.org\" target=\"_blank\"><u>Signal</u></a>&nbsp;&nbsp;\n        \n          <a href=\"https://kiwiirc.com/\" target=\"_blank\"><u>IRC</u></a>&nbsp;&nbsp;\n\n    </div><br>\n\t</div>\n    <!-- end communications -->\n    \n    \n    <!-- start email -->\n\t<div class=\"area\">\n    <h3>Email Provider<small>&nbsp;&nbsp;(like Gmail, Yahoo, etc)</small></h3>\n    <div class=\"box\">\n          <a href=\"https://protonmail.com\" target=\"_blank\"><u>ProtonMail</u></a>&nbsp;&nbsp;\n        \n          <a href=\"https://startmail.com/\" target=\"_blank\"><u>StartMail</u></a>&nbsp;&nbsp;\n\n          <a href=\"https://tutanota.com/\" target=\"_blank\"><u>Tutanota</u></a>&nbsp;&nbsp;\n        \n          <a href=\"https://disroot.org/en\" target=\"_blank\"><u>Disroot</u></a>&nbsp;&nbsp;\n\n          <a href=\"https://mailfence.com/\" target=\"_blank\"><u>Mailfence</u></a>&nbsp;&nbsp;\n        \n        \n    </div><br>\n\t</div>\n    <!-- end email -->\n\n    <!-- start email alias -->\n\t<div class=\"area\">\n    <h3>Email Forwarding<small>&nbsp;&nbsp;(mask your email address)</small></h3>\n    <div class=\"box\">\n          <a href=\"https://anonaddy.com/\" target=\"_blank\"><u>AnonAddy</u></a>&nbsp;&nbsp;\n        \n          <a href=\"https://simplelogin.io/\" target=\"_blank\"><u>SimpleLogin</u></a>&nbsp;&nbsp;\n        \n    </div><br>\n\t</div>\n    <!-- end email alias -->\n    \n    <!-- start social media -->\n\t<div class=\"area\">\n    <h3>Social Media<small>&nbsp;&nbsp;(microblogging aka like twitter)</small></h3>\n    <div class=\"box\">\n          <a href=\"https://mastodon.social\" target=\"_blank\"><u>Mastodon</u></a>&nbsp;&nbsp;\n        \n          <a href=\"https://diasporafoundation.org/\" target=\"_blank\"><u>Diaspora</u></a>&nbsp;&nbsp;\n        \n          <a href=\"https://gab.com\" target=\"_blank\"><u>Gab Social</u></a>&nbsp;&nbsp;\n    </div><br>\n\t</div>\n    <!-- end social media -->\n    \n    <!-- start OS -->\n\t<div class=\"area\">\n    <h3>Operating System<small>&nbsp;&nbsp;(kinda like windows)</small></h3>\n    <div class=\"box\">\n          <a href=\"https://ubuntu.com\" target=\"_blank\"><u>Ubuntu</u>&nbsp;&nbsp;</a>&nbsp;&nbsp;\n        \n         <a href=\"https://zorin.com\" target=\"_blank\"><u>Zorin OS</u>&nbsp;&nbsp;</a>&nbsp;&nbsp;\n        \n         <a href=\"https://draugeros.org\" target=\"_blank\"><u>DraugerOS</u></a>&nbsp;&nbsp;\n\n         <a href=\"https://tails.boum.org/\" target=\"_blank\"><u>Tails OS</u></a>&nbsp;&nbsp;\n    </div><br>\n\t</div>\n    <!-- end OS -->\n    \n     <!-- start browser -->\n\t <div class=\"area\">\n    <h3>Browser<small>&nbsp;&nbsp;(stop using Chrome already)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://torproject.org\" target=\"_blank\"><u>Tor</u></a>&nbsp;&nbsp;\n        \n         <a href=\"https://firefox.com\" target=\"_blank\"><u>Firefox</u></a>&nbsp;&nbsp;\n        \n         <a href=\"https://www.waterfox.net/\" target=\"_blank\"><u>Waterfox</u></a>&nbsp;&nbsp;\n    </div><br>\n\t</div>\n    <!-- end browser -->\n\t\n\t <!-- start PS -->\n\t <div class=\"area\">\n    <h3>Password Manager<small>&nbsp;&nbsp;(stop trying to remember your passwords)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://levin.my.to\" target=\"_blank\"><u>Levin</u></a>&nbsp;&nbsp;\n         <a href=\"https://lesspass.com\" target=\"_blank\"><u>LessPass</u></a>&nbsp;&nbsp;\n         <a href=\"https://bitwarden.com/\" target=\"_blank\"><u>Bitwarden</u></a>&nbsp;&nbsp;\n       \n    </div><br>\n\t</div>\n    <!-- end PS -->\n\t\n\t <!-- start VPN -->\n\t <div class=\"area\">\n    <h3>VPN Software<small>&nbsp;&nbsp;(hide &amp; protect your public IP Address)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://protonvpn.com\" target=\"_blank\"><u>ProtonVPN</u></a>&nbsp;&nbsp;\n         <a href=\"https://mullvad.net\" target=\"_blank\"><u>Mullvad</u></a>&nbsp;&nbsp;\n         <a href=\"https://njal.la/vpn/\" target=\"_blank\"><u>Njalla-VPN</u></a>&nbsp;&nbsp;\n       \n    </div><br>\n\t</div>\n    <!-- end VPN -->\n\n    <!-- start DNS -->\n\t <div class=\"area\">\n    <h3>DNS Resolvers<small>&nbsp;&nbsp;(alternatives to Google DNS)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://adguard.com/en/adguard-dns/overview.html\" target=\"_blank\"><u>AdGuard</u></a>&nbsp;&nbsp;\n         <a href=\"https://blahdns.de/\" target=\"_blank\"><u>BlahDNS</u></a>&nbsp;&nbsp;\n         <a href=\"https://1.1.1.1/dns/\" target=\"_blank\"><u>1.1.1.1</u></a>&nbsp;&nbsp;\n         <a href=\"https://libredns.gr//\" target=\"_blank\"><u>LibreDNS</u></a>&nbsp;&nbsp;\n         <a href=\"https://nextdns.io/\" target=\"_blank\"><u>NextDNS</u></a>&nbsp;&nbsp;\n         <a href=\"https://quad9.net/\" target=\"_blank\"><u>Quad9</u></a>&nbsp;&nbsp;\n       \n    </div><br>\n\t</div>\n    <!-- end DNS -->\n\t\t\n   <!-- start host -->\n   <div class=\"area\">\n    <h3>Hosting Providers<small>&nbsp;&nbsp;(similar to HostGator - <a href=\"https://swivro.net/safe-hosting-providers.html\"><u>learn more</u></a>)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://webdock.io\" target=\"_blank\"><u>Webdock</u></a>&nbsp;&nbsp;\n         <a href=\"https://njal.la\" target=\"_blank\"><u>Njalla</u></a>&nbsp;&nbsp;\n         <a href=\"https://www.bahnhof.net/\" target=\"_blank\"><u>Bahnhof</u></a>&nbsp;&nbsp;\n         <a href=\"https://www.1984hosting.com/\" target=\"_blank\"><u>1984 Hosting</u></a>&nbsp;&nbsp;\n       \n    </div><br>\n\t</div>\n    <!-- end host -->\n\t\n\t\n\t <!-- start files -->\n\t <div class=\"area\">\n    <h3>Cloud File Storage<small>&nbsp;&nbsp;(like Google Drive &amp; Dropbox)</small></h3>\n    <div class=\"box\">\n     <a href=\"https://nextcloud.com\" target=\"_blank\"><u>Nextcloud</u></a>&nbsp;&nbsp; \n\n     <a href=\"https://protonmail.com/blog/proton-drive-early-access/\" target=\"_blank\"><u>Proton Drive</u></a>&nbsp;&nbsp; \n\n\t\t <a href=\"https://sync.com\" target=\"_blank\"><u>Sync</u></a>&nbsp;&nbsp;\n       \n    </div><br>\n\t</div>\n    <!-- end files -->\n    \n     <!-- start suite -->\n\t <div class=\"area\">\n    <h3>Spreadsheets/Doc Editing<small>&nbsp;&nbsp;(sort of like microsoft office &amp; google docs)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://libreoffice.org\" target=\"_blank\"><u>LibreOffice</u></a>&nbsp;&nbsp;\n\n         <a href=\"https://cryptpad.fr/\" target=\"_blank\"><u>CryptPad</u></a>&nbsp;&nbsp;\n    </div><br>\n\t</div>\n    <!-- end suite -->\n    \n    <!-- start SE -->\n\t <div class=\"area\">\n    <h3>Search Engine<small>&nbsp;&nbsp;(google/bing alternatives)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://duckduckgo.com\" target=\"_blank\"><u>DuckDuckGo</u></a>&nbsp;&nbsp;\n         <a href=\"https://qwant.com\" target=\"_blank\"><u>Qwant</u></a>&nbsp;&nbsp;\n         <a href=\"https://startpage.com\" target=\"_blank\"><u>Startpage</u></a>&nbsp;&nbsp;\n         <a href=\"https://www.mojeek.com/\" target=\"_blank\"><u>Mojeek</u></a>&nbsp;&nbsp;\n         <a href=\"https://searx.nixnet.services\" target=\"_blank\"><u>Searx</u></a>&nbsp;&nbsp;\n    </div><br>\n\t</div>\n    <!-- end SE -->\n\n    <!-- start crypto exchanges -->\n\t <div class=\"area\">\n    <h3>Crypto Exchanges<small>&nbsp;&nbsp;(exchange fiat for crypto, vice-versa, or crypto for crypto)</small></h3>\n    <div class=\"box\">\n         <a href=\"https://binance.com\" target=\"_blank\"><u>Binance</u></a>&nbsp;&nbsp;\n         <a href=\"https://bisq.network/\" target=\"_blank\"><u>Bisq</u></a>&nbsp;&nbsp;\n         <a href=\"https://swapzone.io\" target=\"_blank\"><u>Swapzone (crypto 4 crypto)</u></a>&nbsp;&nbsp;\n    </div><br>\n\t</div>\n    <!-- end crypto exchanges -->\n\n</div>\n        </div>\n    \n</section>",
